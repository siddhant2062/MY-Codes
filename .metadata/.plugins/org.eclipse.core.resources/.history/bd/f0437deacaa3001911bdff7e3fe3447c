import java.util.Scanner;
public class averagemarks {
	
	

	private static Scanner s;

	public static void main(String args[]) {
	s = new Scanner(System.in);
	
    String str=s.next();
	int t1=s.nextInt();
	int t2=s.nextInt();
	int t3=s.nextInt();
	
	float avg=(t1+t2+t3)/3;
	
	System.out.println(str);
	System.out.println(avg);
	
		
	}

}
