//product of two numbers=hcf*lcm 
package contest_1;
import java.util.*;
public class v_lcm_of_two_no {
public static void main(String args[]) {
	Scanner s=new Scanner(System.in);
	int n1=s.nextInt();
	int n2=s.nextInt();
	int gcd=1;
	for(int i=2;i<=n1;i++){
		if(n1%i==0&&n2%i==0) {
			gcd=i;
		}
	}
	int lcm=(n1*n2)/gcd;
	System.out.println(gcd);//hcf of two no
	System.out.println(lcm);//lcm of two no
}
}
